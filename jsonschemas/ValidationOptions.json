{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "$ref": "#/$defs/ValidationOptions",
    "$defs": {
        "ValidationOptions": {
            "properties": {
                "stringWithLengthConstraints": {
                    "type": "string",
                    "maxLength": 10,
                    "minLength": 5
                },
                "luckyNumbersWithArrayConstraints": {
                    "items": {
                        "type": "integer"
                    },
                    "type": "array",
                    "maxItems": 6,
                    "minItems": 2
                }
            },
            "additionalProperties": true,
            "type": "object",
            "title": "Validation Options"
        }
    }
}